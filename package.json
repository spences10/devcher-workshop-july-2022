    "daisyui": "^2.19.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.5",
    "theme-change": "^2.1.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13"
